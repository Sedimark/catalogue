@prefix fuseki: <http://jena.apache.org/fuseki#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

fuseki:serviceConfig
    a fuseki:Service ;
    fuseki:servicePath "dataset" ;
    fuseki:dataset [
        a fuseki:Dataset ;
        fuseki:graph "http://example.org/graph" ;
        fuseki:read true ;
        fuseki:write true ;
        fuseki:auth "none" ;
    ] .